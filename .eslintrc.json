{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-tabs": "off",
    "indent": ["error", "tab"],
    "object-curly-newline": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
